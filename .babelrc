{
    "presets": [
        "react",
        "es2015"
    ],
    "plugins": [
        "react-hot-loader/babel",
        "transform-class-properties",
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "<components>": "./src/Components"
                }
            }
        ]
    ]
}